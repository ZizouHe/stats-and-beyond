{
    "title": "Statistics and Beyond",
    "plugins": [
        "mathjax",
        "image-captions",
        "github",
        "anchors",
        "anchor-navigation-ex",
        "-sharing",
        "sharing-plus@^0.0.2",
        "github-buttons",
        "embed-pdf"
    ],

    "pluginsConfig": {
        "image-captions": {
            "caption": "Figure _PAGE_IMAGE_NUMBER_. _CAPTION_",
            "align": "right"
        },
        "github": {
            "url": "https://github.com/ZizouHe/"
        },
        "anchor-navigation-ex": {
            "multipleH1": false,
            "mode": "float",
            "float": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "sharing": {
           "douban": false,
           "facebook": true,
           "google": false,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": false,
           "linkedin": false,
           "messenger": false,
           "pocket": false,
           "qq": true,
           "qzone": false,
           "stumbleupon": false,
           "twitter": false,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": false,
           "all": [
               "facebook", "google", "twitter",
               "weibo", "qq", "linkedin"
           ]
       },
        "github-buttons": {
              "buttons": [{
                "user": "ZizouHe",
                "repo": "stats-and-beyond",
                "type": "star",
                "size": "small"
              }]
            }
    },
    "pdf": {
        "pageNumbers": true,
        "fontFamily": "Arial",
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
          "right": 62,
          "left": 62,
          "top": 56,
          "bottom": 56
        }
    }
}

